<h1>Create a poem</h1>

<form action="/poems" method="POST">
	<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
	<select name="id">
	<% @twitter_handles.each do |handle| %>
	<option value="<%= handle.id %>"><%= handle.handle %></option>
	<% end %>
	</select>
	<input type="submit">
</form>

